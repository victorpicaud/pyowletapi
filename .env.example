# Owlet MCP Server Configuration
# Copy this file to .env and fill in your Owlet account credentials

# Owlet Account Credentials
OWLET_USER=your_email@example.com
OWLET_PASSWORD=your_password

# Region Configuration
# Options: "world" (default, for US/Canada/etc.) or "europe" (for European accounts)
OWLET_REGION=world

# Optional: Logging Configuration
LOG_LEVEL=INFO

# Optional: API Rate Limiting (requests per minute)
API_RATE_LIMIT=60

# Optional: Cache Settings (in seconds)
DEVICE_CACHE_TTL=300
PROPERTIES_CACHE_TTL=10